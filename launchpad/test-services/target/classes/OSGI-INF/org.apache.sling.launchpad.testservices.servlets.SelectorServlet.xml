<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component name="org.apache.sling.launchpad.testservices.servlets.SelectorServlet">
        <implementation class="org.apache.sling.launchpad.testservices.servlets.SelectorServlet"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.resourceTypes">
            sling/servlet/default
        </property>
        <property name="sling.servlet.selectors">
            TEST_SEL_1
            TEST_SEL_2
        </property>
        <property name="sling.servlet.extensions">
            txt
        </property>
        <property name="sling.servlet.methods">
            GET
            POST
        </property>
        <property name="service.vendor" value="The Apache Software Foundation"/>
        <property name="service.pid" value="org.apache.sling.launchpad.testservices.servlets.SelectorServlet"/>
    </scr:component>
</components>
